<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net48</TargetFramework>
		<AssemblyName>InSimDotNet</AssemblyName>
		<RootNamespace>InSimDotNet</RootNamespace>
		<SignAssembly>true</SignAssembly>
		<AssemblyOriginatorKeyFile>InSimDotNet.snk</AssemblyOriginatorKeyFile>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<AssemblyVersion>2.4.5.2</AssemblyVersion>
		<FileVersion>2.4.5.2</FileVersion>
		<PackageLicenseFile>license.txt</PackageLicenseFile>
		<NeutralLanguage>en</NeutralLanguage>
		<Title>InSim.NET</Title>
		<PackageProjectUrl>https://github.com/alexmcbride/insimdotnet/</PackageProjectUrl>
		<Version>$(AssemblyVersion)</Version>
		<Authors>AlexMcBride</Authors>
		<RepositoryUrl>https://github.com/alexmcbride/insimdotnet</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\license.txt">
		  <Pack>True</Pack>
		  <PackagePath>\</PackagePath>
		</None>
		<None Include="README.md" Pack="true" PackagePath="README.md" />
	</ItemGroup>

	<ItemGroup>
	  <Folder Include="Properties\" />
	</ItemGroup>

</Project>